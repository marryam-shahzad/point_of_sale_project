<h1>Prices</h1>

<table>
  <thead>
    <tr>
      <th>Product ID</th>
      <th>Price</th>
    </tr>
  </thead>
  <tbody>
    <% @prices.each do |price| %>
      <tr>
        <td><%= price.product_id %></td>
        <td><%= price.price %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New Price', new_store_product_price_path(@store, @product) %>
<%= link_to 'Back to Product', store_product_path(@store, @product) %>
